
.globl foo
foo:
	push %ebp
	mov %esp, %ebp
	jmp L1

	addl $40, %esp

	movl $0, 4(%ebp)
	movl $0, 8(%ebp)
	movl $0, 12(%ebp)
	jmp L1

	movl $0, %ecx
	movl $1, %ecx
	movl $10, %ecx
	movl -4(%ebp), %ecx
	movl 8(%ebp), %ecx

	addl $0, %ecx
	addl $1, %ecx
	addl $10, %ecx
	addl -4(%ebp), %ecx
	addl 8(%ebp), %ecx

	subl $0, %ecx
	subl $1, %ecx
	subl $10, %ecx
	subl -4(%ebp), %ecx
	subl 8(%ebp), %ecx

	imul $0, %ecx
	imul $1, %ecx
	imul $10, %ecx
	imul -4(%ebp), %ecx
	imul 8(%ebp), %ecx

	jmp L1


	cmpl $0, 4(%ebp)
	je L1
L1:
	mov %ebp, %esp
	pop %ebp
	ret
	jmp L1
